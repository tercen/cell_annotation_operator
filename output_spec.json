{
  "outputSpecs": [{
    "kind": "OperatorJoinSpec",
    "description": "Cluster annotation with best match and detailed probabilities",
    "joinOperators": [
      {
        "kind": "JoinOperator",
        "joinType": "left",
        "leftPair": {
          "kind": "ColumnPair",
          "lColumns": ["Cluster"],
          "rColumns": ["Cluster"]
        },
        "rightRelation": {
          "kind": "TableRelation",
          "description": "Best annotation per cluster",
          "attributes": [
            {"kind": "Attribute", "name": "pop.name", "type": "string"},
            {"kind": "Attribute", "name": "best.pop.pvalue", "type": "double"},
            {"kind": "Attribute", "name": "best.pop.probability", "type": "double"},
            {"kind": "Attribute", "name": "pop.list", "type": "string"}
          ]
        }
      },
      {
        "kind": "JoinOperator",
        "joinType": "left",
        "leftPair": {
          "kind": "ColumnPair",
          "lColumns": ["Cluster"],
          "rColumns": ["Cluster"]
        },
        "rightRelation": {
          "kind": "TableRelation",
          "description": "Detailed probability per cluster-population pair",
          "attributes": [
            {"kind": "Attribute", "name": "pop.name", "type": "string"},
            {"kind": "Attribute", "name": "best.pop.probability", "type": "double"},
            {"kind": "Attribute", "name": "best.pop.pvalue", "type": "double"}
          ]
        }
      }
    ]
  }]
}
